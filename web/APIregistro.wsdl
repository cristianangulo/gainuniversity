<?xml version="1.0" encoding="ISO-8859-1"?>
<definitions xmlns:SOAP-ENV="http://schemas.xmlsoap.org/soap/envelope/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:SOAP-ENC="http://schemas.xmlsoap.org/soap/encoding/"
    xmlns:tns="urn:arnleadservicewsdl"
    xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    targetNamespace="urn:registroSoapservicewsdl">

    <types>
        <xsd:schema targetNamespace="urn:registroSoapwsdl">
            <xsd:import namespace="http://schemas.xmlsoap.org/soap/encoding/" />
            <xsd:import namespace="http://schemas.xmlsoap.org/wsdl/" />
        </xsd:schema>
    </types>

    <message name="registroSoapRequest">
        <part name="datos" type="xsd:string" />
    </message>

    <message name="registroSoapResponse">
        <part name="return" type="xsd:string" />
    </message>

    <portType name="registroSoapwsdlPortType">
        <operation name="registroSoap">
            <documentation>registroSoap World</documentation>
            <input message="tns:registroSoapRequest"/>
            <output message="tns:registroSoapResponse"/>
        </operation>
    </portType>

    <binding name="registroSoapwsdlBinding" type="tns:registroSoapwsdlPortType">
        <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="registroSoap">
            <soap:operation soapAction="urn:arnleadservicewsdl#registroSoap" style="rpc"/>

            <input>
                <soap:body use="encoded" namespace="urn:registroSoapwsdl"
                    encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </input>

            <output>
                <soap:body use="encoded" namespace="urn:registroSoapwsdl"
                    encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </output>
        </operation>
    </binding>

    <service name="registroSoapwsdl">
        <port name="registroSoapwsdlPort" binding="tns:registroSoapwsdlBinding">
            <soap:address location="http://elearn.loc/soap" />
        </port>
    </service>
</definitions>
